<h1>Current Events</h1>
{{#each data}}
    <ul class="list-group">

        <li>
            <h3><a href="/view_event/{{this.id}}">{{this.eventName}}</a></h3></li>
        <h3>{{this.description}}</h3>
    </ul>
{{/each}}
<a href="/new_event" type="button" class="btn btn-primary btn-lg">
  Add An Event
</a>
<a href="/" type="button" class="btn btn-primary btn-lg">
  Back To Home
</a>
